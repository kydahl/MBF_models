\begin_inset Formula 
\begin{align*}
\frac{d}{dt}S_{H} & =\mu_{H}K_{H}-\varPhi_{H}S_{H}-\mu_{H}S_{H}\\
\frac{d}{dt}I_{H} & =\varPhi_{H}S_{H}-\gamma_{H}I_{H}-\mu_{H}I_{H}\\
\frac{d}{dt}\vec{S} & =\left(\rho_{L}L+\gamma S_{V}\right)\begin{bmatrix}1\\
0\\
0\\
0
\end{bmatrix}-\left(\mu I+\boldsymbol{\Phi_{V}}-\boldsymbol{A}^{T}\right)\vec{S}\\
\frac{d}{dt}S_{V} & =p_{G}\lambda_{G}S_{G}-\left(\gamma+\mu\right)S_{V}-\Phi_{V}p_{G}\lambda_{G}S_{G}\\
\frac{d}{dt}\vec{E} & =\boldsymbol{\Phi_{V}}\vec{S}+\gamma E_{V}\begin{bmatrix}1\\
0\\
0\\
0
\end{bmatrix}-\left(\left(\mu+\eta\right)I-\boldsymbol{A}^{T}\right)\vec{E}\\
\frac{d}{dt}E_{V} & =\Phi_{V}p_{G}\lambda_{G}S_{G}+p_{G}\lambda_{G}E_{G}-\left(\gamma+\mu\right)E_{V}-\eta E_{V}\\
\frac{d}{dt}\vec{I} & =\eta\vec{E}+\gamma I_{V}\begin{bmatrix}1\\
0\\
0\\
0
\end{bmatrix}-\left(\mu I-\boldsymbol{A}^{T}\right)\vec{I}\\
\frac{d}{dt}I_{V} & =p_{G}\lambda_{G}I_{G}-\left(\gamma+\mu\right)I_{V}
\end{align*}

