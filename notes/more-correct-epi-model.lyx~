\begin_inset Formula 
\begin{align*}
\frac{d}{dt}S_{H} & =\mu_{H}K_{H}-\varPhi_{H}S_{H}-\mu_{H}S_{H}\\
\frac{d}{dt}I_{H} & =\varPhi_{H}S_{H}-\left(\gamma_{H}+\mu_{H}\right)I_{H}\\
\frac{d}{dt}\vec{S} & =r_{S}\vec{\alpha}_{S}-\boldsymbol{A}_{I}^{T}\vec{S}\\
\frac{d}{dt}\vec{E} & =r_{E}\vec{\alpha}_{E}-\boldsymbol{A}_{E}^{T}\vec{E}\\
\frac{d}{dt}\vec{I} & =r_{I}\vec{\alpha}_{I}-\boldsymbol{A}_{I}^{T}\vec{I}\\
\frac{d}{dt}S_{V} & =p_{G}\lambda_{G}\left(1-\Phi_{V}\right)S_{G}-\left(\gamma+\mu\right)S_{V}\\
\frac{d}{dt}E_{V} & =p_{G}\lambda_{G}\left(\Phi_{V}S_{G}+E_{G}\right)-\left(\eta+\gamma+\mu\right)E_{V}\\
\frac{d}{dt}I_{V} & =p_{G}\lambda_{G}I_{G}-\left(\gamma+\mu\right)I_{V}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
r_{S} & =\left(\rho_{L}L+\gamma S_{V}\right)\\
\vec{\alpha}_{S} & =\begin{bmatrix}1\\
0\\
0\\
0
\end{bmatrix}\\
\boldsymbol{A}_{I} & =\mu I+\boldsymbol{\Phi_{V}}^{T}-\boldsymbol{A}\\
r_{E} & =\Phi_{V}\left(1-p_{G}\right)\lambda_{G}S_{G}+\gamma E_{V}\\
\vec{\alpha}_{E} & =\left[\Phi_{V}\left(1-p_{G}\right)\lambda_{G}S_{G}+\gamma E_{V}\right]^{-1}\left(\boldsymbol{\Phi_{V}}\vec{S}+\gamma E_{V}\vec{\alpha}\right)\\
\boldsymbol{A}_{E} & =\left(\mu+\eta\right)I-\boldsymbol{A}\\
r_{I} & =\eta\left(E_{Q}+E_{L}+E_{P}+E_{G}\right)+\gamma I_{V}\\
\vec{\alpha}_{I} & =\left[\eta\left(E_{Q}+E_{L}+E_{P}+E_{G}\right)+\gamma I_{V}\right]^{-1}\begin{bmatrix}\eta E_{Q}+\gamma I_{V}\\
\eta E_{L}\\
\eta E_{P}\\
\eta E_{G}
\end{bmatrix}\\
\boldsymbol{A}_{I} & =\mu I-\boldsymbol{A}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\boldsymbol{A}_{S}^{T}=\begin{bmatrix}\mu+\lambda_{Q} & -f\left(1-p_{L}\right)\lambda_{L} & -f\left(1-p_{P}\right)\lambda_{P} & -f\left(1-p_{G}\right)\lambda_{G}\left(1-\Phi_{V}\right)\\
-\lambda_{Q} & \mu+\lambda_{L}-\left(1-f\right)\left(1-p_{L}\right)\lambda_{L} & -\left(1-f\right)\left(1-p_{P}\right)\lambda_{P} & -\left(1-f\right)\left(1-p_{G}\right)\lambda_{G}\left(1-\Phi_{V}\right)\\
0 & -p_{L}\lambda_{L} & \mu+\lambda_{P} & -p_{p}\lambda_{P}\\
0 & 0 & 0 & \mu+\lambda_{G}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
-\vec{1}^{T}A_{S}^{T}\vec{S} & =-\vec{1}^{T}\left(\mu I+\boldsymbol{\Phi_{V}}-\boldsymbol{A}^{T}\right)\vec{S}\\
-\vec{1}^{T}A_{S}^{T}\vec{S} & =-\vec{1}^{T}\left(\mu I+\begin{bmatrix}0 & 0 & 0 & \Phi_{V}\left(1-p_{G}\right)\lambda_{G}f\\
0 & 0 & 0 & \Phi_{V}\left(1-p_{G}\right)\lambda_{G}\left(1-f\right)\\
0 & 0 & 0 & 0\\
0 & 0 & 0 & 0
\end{bmatrix}-\begin{bmatrix}-\lambda_{Q} & f\left(1-p_{L}\right)\lambda_{L} & f\left(1-p_{P}\right)\lambda_{P} & f\left(1-p_{G}\right)\lambda_{G}\\
\lambda_{Q} & \left(1-f\right)\left(1-p_{L}\right)\lambda_{L}-\lambda_{L} & \left(1-f\right)\left(1-p_{P}\right)\lambda_{P} & \left(1-f\right)\left(1-p_{G}\right)\lambda_{G}\\
0 & p_{L}\lambda_{L} & -\lambda_{P} & p_{p}\lambda_{P}\\
0 & 0 & 0 & -\lambda_{G}
\end{bmatrix}\right)\vec{S}\\
-\vec{1}^{T}\boldsymbol{A}_{S}^{T}\vec{S} & =-\begin{bmatrix}1 & 1 & 1 & 1\end{bmatrix}\begin{bmatrix}\mu+\lambda_{Q} & -f\left(1-p_{L}\right)\lambda_{L} & -f\left(1-p_{P}\right)\lambda_{P} & -f\left(1-p_{G}\right)\lambda_{G}\left(1-\Phi_{V}\right)\\
-\lambda_{Q} & \mu+\lambda_{L}-\left(1-f\right)\left(1-p_{L}\right)\lambda_{L} & -\left(1-f\right)\left(1-p_{P}\right)\lambda_{P} & -\left(1-f\right)\left(1-p_{G}\right)\lambda_{G}\left(1-\Phi_{V}\right)\\
0 & -p_{L}\lambda_{L} & \mu+\lambda_{P} & 0\\
0 & 0 & -p_{p}\lambda_{P} & \mu+\lambda_{G}
\end{bmatrix}\vec{S}\\
-\vec{1}^{T}\boldsymbol{A}_{S}^{T}\vec{S} & =-\begin{bmatrix}\mu & \mu & \mu & \mu+\lambda_{G}\left[1-\left(1-p_{G}\right)\left(1-\Phi_{V}\right)\right]\end{bmatrix}\vec{S}\\
-\vec{1}^{T}\boldsymbol{A}_{S}^{T}\vec{S} & =-\begin{bmatrix}\mu & \mu & \mu & \mu+p_{G}\lambda_{G}+\Phi_{V}\left(1-p_{G}\right)\lambda_{G}\end{bmatrix}\begin{bmatrix}S_{Q}\\
S_{L}\\
S_{P}\\
S_{G}
\end{bmatrix}\\
-\vec{1}^{T}\boldsymbol{A}_{S}^{T}\vec{S} & =-\mu\left(S_{Q}+S_{L}+S_{P}+S_{G}\right)-p_{G}\lambda_{G}S_{G}-\Phi_{V}\left(1-p_{G}\right)\lambda_{G}S_{G}
\end{align*}

