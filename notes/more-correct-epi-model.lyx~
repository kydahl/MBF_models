\begin_inset Formula 
\begin{align*}
\frac{d}{dt}S_{H} & =\mu_{H}K_{H}-\varPhi_{H}S_{H}-\mu_{H}S_{H}\\
\frac{d}{dt}I_{H} & =\varPhi_{H}S_{H}-\gamma_{H}I_{H}-\mu_{H}I_{H}\\
\frac{d}{dt}S_{Q} & =\left(\rho_{L}L+\gamma S_{V}\right)+f\left[\left(1-p_{L}\right)\lambda_{L}S_{L}+\left(1-p_{P}\right)\lambda_{P}S_{P}+\left(1-p_{G}\right)\lambda_{G}S_{G}\right]-\left(\lambda_{Q}+\mu\right)S_{Q}-f\left(1-p_{G}\right)\Phi_{V}\lambda_{G}S_{G}\\
\frac{d}{dt}S_{L} & =\lambda_{Q}S_{Q}+\left(1-f\right)\left[\left(1-p_{P}\right)\lambda_{P}S_{P}+\left(1-p_{G}\right)\lambda_{G}S_{G}\right]-\left[f\left(1-p_{L}\right)+p_{L}+\mu\right]\lambda_{L}S_{L}-\left(1-f\right)\left(1-p_{G}\right)\Phi_{V}\lambda_{G}S_{G}\\
\frac{d}{dt}S_{P} & =p_{L}\lambda_{L}S_{L}-\left(\lambda_{P}+\mu\right)S_{P}\\
\frac{d}{dt}S_{G} & =p_{P}\lambda_{P}S_{P}-\left(\lambda_{G}+\mu\right)S_{G}\\
\frac{d}{dt}S_{V} & =p_{G}\lambda_{G}S_{G}-\left(\gamma+\mu\right)S_{V}-\Phi_{V}p_{G}\lambda_{G}S_{G}\\
\frac{d}{dt}E_{Q} & =-\eta E_{Q}+\gamma E_{V}+f\left[\left(1-p_{L}\right)\lambda_{L}E_{L}+\left(1-p_{P}\right)\lambda_{P}E_{P}+\left(1-p_{G}\right)\lambda_{G}E_{G}\right]-\left(\lambda_{Q}+\mu\right)E_{Q}+f\Phi_{V}\left(1-p_{G}\right)\lambda_{G}S_{G}\\
\frac{d}{dt}E_{L} & =-\eta E_{L}+\lambda_{Q}E_{Q}+\left(1-f\right)\left[\left(1-p_{P}\right)\lambda_{P}E_{P}+\left(1-p_{G}\right)\lambda_{G}E_{G}\right]+\left[\left(1-f\right)\left(1-p_{L}\right)-1-\mu\right]\lambda_{L}E_{L}+\left(1-f\right)\Phi_{V}\left(1-p_{G}\right)\lambda_{G}S_{G}\\
\frac{d}{dt}E_{P} & =-\eta E_{P}+p_{L}\lambda_{L}E_{L}-\left(\lambda_{P}+\mu\right)E_{P}\\
\frac{d}{dt}E_{G} & =-\eta E_{G}+p_{P}\lambda_{P}E_{P}-\left(\lambda_{G}+\mu\right)E_{G}-\mu E_{G}\\
\frac{d}{dt}E_{V} & =-\eta E_{V}+p_{G}\lambda_{G}E_{G}-\left(\gamma+\mu\right)E_{V}+\Phi_{V}p_{G}\lambda_{G}S_{G}\\
\frac{d}{dt}I_{Q} & =\left(\eta E_{Q}+\gamma I_{V}\right)+f\left[\left(1-p_{L}\right)\lambda_{L}I_{L}+\left(1-p_{P}\right)\lambda_{P}I_{P}\right]-\left(\lambda_{Q}+\mu\right)I_{Q}\\
\frac{d}{dt}I_{L} & =\eta E_{L}+\lambda_{Q}I_{Q}+\left(1-f\right)\left[\left(1-p_{P}\right)\lambda_{P}I_{P}+\left(1-p_{G}\right)\lambda_{G}I_{G}\right]-\left[f\left(1-p_{L}\right)+p_{L}+\mu\right]\lambda_{L}I_{L}\\
\frac{d}{dt}I_{P} & =\eta E_{P}+p_{L}\lambda_{L}I_{L}-\left(\lambda_{P}+\mu\right)I_{P}\\
\frac{d}{dt}I_{G} & =\eta E_{G}+p_{P}\lambda_{P}I_{P}-\left(\lambda_{G}+\mu\right)I_{G}\\
\frac{d}{dt}I_{V} & =p_{G}\lambda_{G}I_{G}-\left(\gamma+\mu\right)I_{V}
\end{align*}

