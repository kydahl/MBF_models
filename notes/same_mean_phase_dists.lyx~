\end_inset


\begin_inset Formula 
\begin{align*}
a_{2}-a_{1} & =\frac{1}{\lambda_{Q}}\\
a_{2} & =\frac{1}{\lambda_{Q}}+a_{1}\\
a_{1}f\left(1-p_{L}\right)-a_{2}\left[f\left(1-p_{L}\right)+p_{L}\right]+p_{L}a_{3} & =0\\
-a_{2}p_{L}+p_{L}a_{3} & =\frac{f\left(1-p_{L}\right)}{\lambda_{Q}}\\
p_{L}a_{3} & =\frac{f\left(1-p_{L}\right)}{\lambda_{Q}}+a_{2}p_{L}\\
a_{3} & =\frac{f\left(1-p_{L}\right)}{p_{L}\lambda_{Q}}+\frac{1}{\lambda_{Q}}+a_{1}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
a_{1}f\left(1-p_{P}\right)+a_{2}\left(1-f\right)\left(1-p_{P}\right)-a_{3}+p_{P}a_{4} & =0\\
-\frac{f\left(1-p_{P}\right)}{\lambda_{Q}}+a_{2}-a_{3}+p_{P}a_{4} & =0\\
a_{4} & =\frac{f\left(1-p_{P}\right)}{p_{P}\lambda_{Q}}-\frac{1}{p_{P}}a_{2}+\frac{1}{p_{P}}a_{3}\\
a_{4} & =\frac{f\left(1-p_{P}\right)}{p_{P}\lambda_{Q}}-\frac{1}{p_{P}}\left(\frac{1}{\lambda_{Q}}+a_{1}\right)+\frac{1}{p_{P}}\left(\frac{f\left(1-p_{L}\right)}{p_{L}\lambda_{Q}}+\frac{1}{\lambda_{Q}}+a_{1}\right)\\
a_{4} & =\frac{1}{p_{P}}f\frac{1}{\lambda_{Q}}\left[\left(1-p_{P}\right)+\frac{\left(1-p_{L}\right)}{p_{L}}\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
a_{1}f\left(1-p_{G}\right)+a_{2}\left(1-f\right)\left(1-p_{G}\right)-a_{4} & =0\\
-\frac{1}{\lambda_{Q}}f\left(1-p_{G}\right)+a_{2}-a_{4} & =0\\
a_{4} & =-\frac{1}{\lambda_{Q}}f\left(1-p_{G}\right)+\frac{1}{\lambda_{Q}}+a_{1}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{f\left(1-p_{P}\right)}{p_{P}\lambda_{Q}}-\frac{1}{p_{P}}\frac{1}{\lambda_{Q}}-\frac{1}{p_{P}}a_{1}+\left(\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}\lambda_{Q}}+\frac{1}{p_{P}}\frac{1}{\lambda_{Q}}+a_{1}\frac{1}{p_{P}}\right) & =-\frac{1}{\lambda_{Q}}f\left(1-p_{G}\right)+\frac{1}{\lambda_{Q}}+a_{1}\\
\frac{f\left(1-p_{P}\right)}{p_{P}\lambda_{Q}}+\left(\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}\lambda_{Q}}\right) & =-\frac{1}{\lambda_{Q}}f\left(1-p_{G}\right)+\frac{1}{\lambda_{Q}}+a_{1}\\
-a_{1} & =-\frac{1}{\lambda_{Q}}f\left(1-p_{G}\right)+\frac{1}{\lambda_{Q}}-\frac{f\left(1-p_{P}\right)}{p_{P}\lambda_{Q}}-\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}\lambda_{Q}}\\
a_{1} & =\frac{1}{\lambda_{Q}}\left\{ f\left(1-p_{G}\right)+\frac{f\left(1-p_{P}\right)}{p_{P}}+\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}}-1\right\} 
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
a_{1} & =\frac{1}{\lambda_{Q}}\left\{ f\left(1-p_{G}\right)+\frac{f\left(1-p_{P}\right)}{p_{P}}+\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}}-1\right\} \\
a_{2} & =\frac{1}{\lambda_{Q}}\left\{ f\left(1-p_{G}\right)+\frac{f\left(1-p_{P}\right)}{p_{P}}+\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}}\right\} \\
a_{3} & =\frac{1}{\lambda_{Q}}\left\{ f\left(1-p_{G}\right)+\frac{f\left(1-p_{P}\right)}{p_{P}}+\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}}+\frac{f\left(1-p_{L}\right)}{p_{L}}\right\} \\
a_{4} & =\frac{1}{\lambda_{Q}}\left\{ \frac{f\left(1-p_{P}\right)}{p_{P}}+\frac{1}{p_{P}}\frac{f\left(1-p_{L}\right)}{p_{L}}\right\} 
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
A^{-1} & =\frac{1}{p_{4}}\begin{bmatrix}-\frac{\left(-p_{2}p_{3}p_{4}x+p_{2}p_{3}p_{4}+x\right)}{p_{2}p_{3}r_{1}} & -\frac{1}{p_{2}p_{3}r_{2}} & -\frac{1}{p_{3}r_{3}} & -\frac{1}{r_{4}}\\
-\frac{\left(x-p_{2}p_{3}p_{4}x\right)}{p_{2}p_{3}r_{1}} & -\frac{1}{p_{2}p_{3}r_{2}} & -\frac{1}{p_{3}r_{3}} & -\frac{1}{r_{4}}\\
-\frac{\left(x-p_{3}p_{4}x\right)}{p_{2}p_{3}r_{1}} & -\frac{\left(1-p_{3}p_{4}\right)}{p_{2}p_{3}r_{2}} & -\frac{1}{p_{3}r_{3}} & -\frac{1}{r_{4}}\\
-\frac{\left(x-p_{4}x\right)}{p_{2}p_{3}r_{1}} & -\frac{\left(1-p_{4}\right)}{p_{2}p_{3}r_{2}} & -\frac{\left(1-p_{4}\right)}{p_{3}r_{3}} & -\frac{1}{r_{4}}
\end{bmatrix}\\
 & =\frac{1}{p_{G}}\begin{bmatrix}-\frac{\left(-p_{L}p_{P}p_{G}f+p_{L}p_{P}p_{G}+f\right)}{p_{L}p_{P}\lambda_{Q}} & -\frac{1}{p_{L}p_{P}\lambda_{L}} & -\frac{1}{p_{P}\lambda_{P}} & -\frac{1}{\lambda_{G}}\\
-\frac{f\left(1-p_{L}p_{P}p_{G}\right)}{p_{L}p_{P}\lambda_{Q}} & -\frac{1}{p_{L}p_{P}\lambda_{L}} & -\frac{1}{p_{P}\lambda_{P}} & -\frac{1}{\lambda_{G}}\\
-\frac{f\left(1-p_{P}p_{G}\right)}{p_{L}p_{P}\lambda_{Q}} & -\frac{\left(1-p_{P}p_{G}\right)}{p_{L}p_{P}\lambda_{L}} & -\frac{1}{p_{P}\lambda_{P}} & -\frac{1}{\lambda_{G}}\\
-\frac{f\left(1-p_{G}\right)}{p_{L}p_{P}\lambda_{Q}} & -\frac{\left(1-p_{G}\right)}{p_{L}p_{P}\lambda_{L}} & -\frac{\left(1-p_{G}\right)}{p_{P}\lambda_{P}} & -\frac{1}{\lambda_{G}}
\end{bmatrix}\\
 & =\frac{1}{p_{G}}\begin{bmatrix}\left(-p_{L}p_{P}p_{G}f+p_{L}p_{P}p_{G}+f\right) & 1 & 1 & 1\\
f\left(1-p_{L}p_{P}p_{G}\right) & 1 & 1 & 1\\
f\left(1-p_{P}p_{G}\right) & \left(1-p_{P}p_{G}\right) & 1 & 1\\
f\left(1-p_{G}\right) & \left(1-p_{G}\right) & \left(1-p_{G}\right) & 1
\end{bmatrix}\begin{bmatrix}-\frac{1}{p_{L}p_{P}\lambda_{Q}} & 0 & 0 & 0\\
0 & -\frac{1}{p_{L}p_{P}\lambda_{L}} & 0 & 0\\
0 & 0 & -\frac{1}{p_{P}\lambda_{P}} & 0\\
0 & 0 & 0 & -\frac{1}{\lambda_{G}}
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula $\mathbb{E}\left(\tau\right)=\vec{1}^{T}\left(-\boldsymbol{A}\right)^{-1}\vec{\alpha}$
\end_inset

 where in our case 
\begin_inset Formula $\vec{\alpha}=\begin{pmatrix}1 & 0 & 0 & 0\end{pmatrix}^{T}$
\end_inset

 and 
\begin_inset Formula $\vec{1}=\begin{pmatrix}1 & 1 & 1 & 1\end{pmatrix}^{T}$
\end_inset


\begin_inset Formula 
\begin{align*}
\mathbb{E}\left(\tau\right) & =\begin{bmatrix}1 & 0 & 0 & 0\end{bmatrix}\frac{1}{p_{G}}\begin{bmatrix}\frac{\left(-p_{L}p_{P}p_{G}f+p_{L}p_{P}p_{G}+f\right)}{p_{L}p_{P}\lambda_{Q}} & \frac{1}{p_{L}p_{P}\lambda_{L}} & \frac{1}{p_{P}\lambda_{P}} & \frac{1}{\lambda_{G}}\\
\frac{f\left(1-p_{L}p_{P}p_{G}\right)}{p_{L}p_{P}\lambda_{Q}} & \frac{1}{p_{L}p_{P}\lambda_{L}} & \frac{1}{p_{P}\lambda_{P}} & \frac{1}{\lambda_{G}}\\
\frac{f\left(1-p_{P}p_{G}\right)}{p_{L}p_{P}\lambda_{Q}} & \frac{\left(1-p_{P}p_{G}\right)}{p_{L}p_{P}\lambda_{L}} & \frac{1}{p_{P}\lambda_{P}} & \frac{1}{\lambda_{G}}\\
\frac{f\left(1-p_{G}\right)}{p_{L}p_{P}\lambda_{Q}} & \frac{\left(1-p_{G}\right)}{p_{L}p_{P}\lambda_{L}} & \frac{\left(1-p_{G}\right)}{p_{P}\lambda_{P}} & \frac{1}{\lambda_{G}}
\end{bmatrix}\begin{bmatrix}1\\
1\\
1\\
1
\end{bmatrix}\\
\mathbb{E}\left(\tau\right) & =\frac{1}{p_{G}}\begin{bmatrix}\frac{\left(-p_{L}p_{P}p_{G}f+p_{L}p_{P}p_{G}+f\right)}{p_{L}p_{P}\lambda_{Q}} & \frac{1}{p_{L}p_{P}\lambda_{L}} & \frac{1}{p_{P}\lambda_{P}} & \frac{1}{\lambda_{G}}\end{bmatrix}\begin{bmatrix}1\\
1\\
1\\
1
\end{bmatrix}\\
 & =\frac{1}{p_{G}}\frac{\left(-p_{L}p_{P}p_{G}f+p_{L}p_{P}p_{G}+f\right)}{p_{L}p_{P}\lambda_{Q}}+\frac{1}{p_{G}}\frac{1}{p_{L}p_{P}\lambda_{L}}+\frac{1}{p_{G}}\frac{1}{p_{P}\lambda_{P}}+\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\\
 & =\frac{1}{p_{L}p_{P}p_{G}}\left\{ \left[1-\left(1-f\right)\left(1-p_{L}p_{P}p_{G}\right)\right]\frac{1}{\lambda_{Q}}+\frac{1}{\lambda_{L}}+p_{L}\frac{1}{\lambda_{P}}+p_{L}p_{P}\frac{1}{\lambda_{G}}\right\} \\
 & =\frac{1}{p_{G}}\frac{-p_{L}p_{P}p_{G}f}{p_{L}p_{P}\lambda_{Q}}+\frac{1}{p_{G}}\frac{p_{L}p_{P}p_{G}}{p_{L}p_{P}\lambda_{Q}}+\frac{1}{p_{G}}\frac{f}{p_{L}p_{P}\lambda_{Q}}+\frac{1}{p_{G}}\frac{1}{p_{L}p_{P}\lambda_{L}}+\frac{1}{p_{G}}\frac{1}{p_{P}\lambda_{P}}+\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\\
 & =\left[\left(1-f\right)+\frac{f}{p_{L}p_{P}p_{G}}\right]\frac{1}{\lambda_{Q}}+\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}+\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}+\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\\
 & =\left[\left(1-f\right)+f\left(\frac{1}{p_{L}}\right)\left(\frac{1}{p_{P}}\right)\left(\frac{1}{p_{G}}\right)\right]\left(\frac{1}{\lambda_{Q}}\right)+\left(\frac{1}{p_{L}}\right)\left(\frac{1}{p_{P}}\right)\left(\frac{1}{p_{G}}\right)\left(\frac{1}{\lambda_{L}}\right)+\left(\frac{1}{p_{P}}\right)\left(\frac{1}{p_{G}}\right)\left(\frac{1}{\lambda_{P}}\right)+\left(\frac{1}{p_{G}}\right)\left(\frac{1}{\lambda_{G}}\right)\\
 & =\frac{1}{p_{L}p_{P}p_{G}}\left[\left(\left(1-f\right)p_{L}p_{P}p_{G}\frac{1}{\lambda_{Q}}+f\right)\frac{1}{\lambda_{Q}}+\frac{1}{\lambda_{L}}+p_{L}\frac{1}{\lambda_{P}}+p_{L}p_{P}\frac{1}{\lambda_{G}}\right]
\end{align*}

\end_inset

Let 
\begin_inset Formula $E\left(\lambda_{Q},f\right)=\mathbb{E}\left(\tau\right)$
\end_inset

.
 Fix 
\begin_inset Formula $\theta=E\left(\lambda_{Q},f\right)$
\end_inset

 and solve for 
\begin_inset Formula $f$
\end_inset

 in terms of 
\begin_inset Formula $\theta$
\end_inset

 and 
\begin_inset Formula $\lambda_{Q}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\left[\left(1-f\right)+\frac{f}{p_{L}p_{P}p_{G}}\right]\frac{1}{\lambda_{Q}}+\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}+\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}+\frac{1}{p_{G}}\frac{1}{\lambda_{G}} & =\theta\\
f & =\left\{ \lambda_{Q}\left[\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right]-1\right\} \frac{p_{L}p_{P}p_{G}}{1-p_{L}p_{P}p_{G}}\\
\frac{1}{\lambda_{Q}} & =\left[\theta-\left(\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}+\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}+\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right)\right]\left[\left(1-f\right)+\frac{f}{p_{L}p_{P}p_{G}}\right]^{-1}\\
\end{align*}

\end_inset

By fixing 
\begin_inset Formula $\theta,p_{L},p_{P},p_{G},\lambda_{L},\lambda_{P},\lambda_{G}$
\end_inset

, we can explore how varying the questing duration (while fixing mean duration
 through 
\begin_inset Formula $f$
\end_inset

) impacts 
\begin_inset Formula $R$
\end_inset

 and 
\begin_inset Formula $\mathcal{R}_{0}$
\end_inset

.
 Notice we must maintain the following bounds on 
\begin_inset Formula $\lambda_{Q}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\left\{ \lambda_{Q}\left[\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right]-1\right\} \frac{p_{L}p_{P}p_{G}}{1-p_{L}p_{P}p_{G}} & <1\\
\lambda_{Q}\left[\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right]p_{L}p_{P}p_{G}-p_{L}p_{P}p_{G} & <1-p_{L}p_{P}p_{G}\\
\lambda_{Q}\left[\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right]p_{L}p_{P}p_{G} & <1\\
\lambda_{Q} & <\frac{1}{p_{L}p_{P}p_{G}}\left(\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right)^{-1}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\lambda_{Q}\left[\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right] & >1\\
\lambda_{Q} & >\left(\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right)^{-1}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\left(\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right)^{-1}<\lambda_{Q}<\frac{1}{p_{L}p_{P}p_{G}}\left(\theta-\frac{1}{p_{L}p_{P}p_{G}}\frac{1}{\lambda_{L}}-\frac{1}{p_{P}p_{G}}\frac{1}{\lambda_{P}}-\frac{1}{p_{G}}\frac{1}{\lambda_{G}}\right)^{-1}
\]

\end_inset


\begin_inset Formula 
