Let 
\begin_inset Formula $\boldsymbol{W}=\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}$
\end_inset

 with
\begin_inset Formula 
\begin{align*}
\boldsymbol{F}_{VH} & =\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\\
\boldsymbol{0}_{1\times n} & \boldsymbol{0}_{1\times n}
\end{bmatrix}\\
\boldsymbol{F}_{HV} & =\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*} & \boldsymbol{0}_{n\times1}\\
\boldsymbol{0}_{n\times1} & \boldsymbol{0}_{n\times1}
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR} & =\begin{bmatrix}\begin{bmatrix}\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right) & \boldsymbol{0}_{n\times n}\\
-\eta\boldsymbol{I} & \left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)
\end{bmatrix} & \begin{bmatrix}-\gamma_{V}\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\vec{\alpha} & \boldsymbol{0}_{n\times1}\\
-\gamma_{V}\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha} & -\gamma_{V}\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha}
\end{bmatrix}\\
\begin{bmatrix}-\left(-\boldsymbol{A}\vec{1}\right)^{T} & \boldsymbol{0}_{1\times n}\\
\boldsymbol{0}_{1\times n} & -\left(-\boldsymbol{A}\vec{1}\right)^{T}
\end{bmatrix} & \begin{bmatrix}\left(\mu+\gamma_{V}+\eta\right) & 0\\
-\eta & \left(\mu+\gamma_{V}\right)
\end{bmatrix}
\end{bmatrix}\\
\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR} & =\begin{bmatrix}\boldsymbol{V}_{BB} & \boldsymbol{V}_{RB}\\
\boldsymbol{V}_{BV} & \boldsymbol{V}_{VR}
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{V}_{BB} & =\begin{bmatrix}\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right) & \boldsymbol{0}_{n\times n}\\
-\eta\boldsymbol{I} & \left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)
\end{bmatrix}\\
\boldsymbol{V}_{RB} & =\begin{bmatrix}-\gamma_{V}\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\vec{\alpha} & \boldsymbol{0}_{n\times1}\\
-\gamma_{V}\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha} & -\gamma_{V}\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha}
\end{bmatrix}\\
\boldsymbol{V}_{BV} & =\begin{bmatrix}-\left(-\boldsymbol{A}\vec{1}\right)^{T} & \boldsymbol{0}_{1\times n}\\
\boldsymbol{0}_{1\times n} & -\left(-\boldsymbol{A}\vec{1}\right)^{T}
\end{bmatrix}\\
\boldsymbol{V}_{VR} & =\begin{bmatrix}\left(\mu+\gamma_{V}+\eta\right) & 0\\
-\eta & \left(\mu+\gamma_{V}\right)
\end{bmatrix}
\end{align*}

\end_inset

Then 
\begin_inset Formula 
\[
\left(\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}\right)^{-1}=\begin{bmatrix}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1} & -\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1}\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\\
-\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1} & \boldsymbol{V}_{VR}^{-1}+\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1}\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{V}_{VR}^{-1} & =\begin{bmatrix}\frac{1}{\mu+\gamma_{V}+\eta} & 0\\
\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\frac{1}{\mu+\gamma_{V}} & \frac{1}{\mu+\gamma_{V}}
\end{bmatrix}\\
\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1} & =\begin{bmatrix}-\gamma_{V}\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\vec{\alpha} & \boldsymbol{0}_{n\times1}\\
-\gamma_{V}\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha} & -\gamma_{V}\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha}
\end{bmatrix}\begin{bmatrix}\frac{1}{\mu+\gamma_{V}+\eta} & 0\\
\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\frac{1}{\mu+\gamma_{V}} & \frac{1}{\mu+\gamma_{V}}
\end{bmatrix}\\
\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1} & =\begin{bmatrix}-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\vec{\alpha} & \boldsymbol{0}_{n\times1}\\
-\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\vec{\alpha} & -\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha}
\end{bmatrix}\\
\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}\frac{1}{\mu+\gamma_{V}+\eta} & 0\\
\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\frac{1}{\mu+\gamma_{V}} & \frac{1}{\mu+\gamma_{V}}
\end{bmatrix}\begin{bmatrix}-\left(-\boldsymbol{A}\vec{1}\right)^{T} & \boldsymbol{0}_{1\times n}\\
\boldsymbol{0}_{1\times n} & -\left(-\boldsymbol{A}\vec{1}\right)^{T}
\end{bmatrix}\\
\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}-\frac{1}{\mu+\gamma_{V}+\eta}\left(-\boldsymbol{A}\vec{1}\right)^{T} & \boldsymbol{0}_{1\times n}\\
-\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\frac{1}{\mu+\gamma_{V}}\left(-\boldsymbol{A}\vec{1}\right)^{T} & -\frac{1}{\mu+\gamma_{V}}\left(-\boldsymbol{A}\vec{1}\right)^{T}
\end{bmatrix}\\
\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\vec{\alpha} & \boldsymbol{0}_{n\times1}\\
-\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\vec{\alpha} & -\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha}
\end{bmatrix}\begin{bmatrix}-\left(-\boldsymbol{A}\vec{1}\right)^{T} & \boldsymbol{0}_{1\times n}\\
\boldsymbol{0}_{1\times n} & -\left(-\boldsymbol{A}\vec{1}\right)^{T}
\end{bmatrix}\\
\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\vec{\alpha}\left(-\boldsymbol{A}\vec{1}\right)^{T} & \boldsymbol{0}_{n\times n}\\
\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\vec{\alpha}\left(-\boldsymbol{A}\vec{1}\right)^{T} & \left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\vec{\alpha}\left(-\boldsymbol{A}\vec{1}\right)^{T}
\end{bmatrix}\\
\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S} & \boldsymbol{0}_{n\times n}\\
\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\boldsymbol{S} & \left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}
\end{bmatrix}\\
\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right) & \boldsymbol{0}_{n\times n}\\
-\eta\boldsymbol{I} & \left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)
\end{bmatrix}-\begin{bmatrix}\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S} & \boldsymbol{0}_{n\times n}\\
\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\boldsymbol{S} & \left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}
\end{bmatrix}\\
\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3} & =\begin{bmatrix}\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S} & \boldsymbol{0}_{n\times n}\\
-\left\{ \eta\boldsymbol{I}+\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\boldsymbol{S}\right\}  & \left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}
\end{bmatrix}\\
\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3}\right)^{-1} & =\begin{bmatrix}\left[\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S}\right]^{-1} & \boldsymbol{0}_{n\times n}\\
\left[\left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}\right]^{-1}\left[\eta\boldsymbol{I}+\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\boldsymbol{S}\right]\left[\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S}\right]^{-1} & \left[\left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}\right]^{-1}
\end{bmatrix}\\
\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{V}_{VR}^{-1}\boldsymbol{W}_{3}\right)^{-1} & =\begin{bmatrix}\boldsymbol{\Gamma}_{E} & \boldsymbol{0}_{n\times n}\\
\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E} & \boldsymbol{\Gamma}_{I}
\end{bmatrix}
\end{align*}

\end_inset

where 
\begin_inset Formula $\boldsymbol{S}=\vec{\alpha}\left(-\boldsymbol{A}\vec{1}\right)^{T}$
\end_inset

,
 
\begin_inset Formula $\boldsymbol{\Gamma}_{E}=\left[\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S}\right]^{-1}$
\end_inset

,
 
\begin_inset Formula $\boldsymbol{\Gamma}_{I}=\left[\left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}\right]^{-1}$
\end_inset

,
 
\begin_inset Formula $\boldsymbol{\tau}_{E}=\left[\eta\boldsymbol{I}+\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\boldsymbol{S}\right]\left[\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S}\right]^{-1}$
\end_inset


\begin_inset Formula 
\[
\left(\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}\right)^{-1}=\begin{bmatrix}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1} & -\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1}\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\\
-\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1} & \boldsymbol{V}_{VR}^{-1}+\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1}\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{F}_{VH}\left(\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}\right)^{-1} & =\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H} & 0 & 0\\
\boldsymbol{0}_{1\times n} & \boldsymbol{0}_{1\times n} & 0 & 0
\end{bmatrix}\begin{bmatrix}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1} & -\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1}\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\\
-\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1} & \boldsymbol{V}_{VR}^{-1}+\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\left(\boldsymbol{V}_{BB}-\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}\boldsymbol{V}_{BV}\right)^{-1}\boldsymbol{V}_{RB}\boldsymbol{V}_{VR}^{-1}
\end{bmatrix}\\
\left[\boldsymbol{F}_{VH}\right]_{2\times2n+2}\left[\boldsymbol{W}^{-1}\right]_{2n+2\times2n+2} & =\begin{bmatrix}\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}_{1\times2n} & \begin{bmatrix}0 & 0\end{bmatrix}_{1\times2}\\
\begin{bmatrix}\boldsymbol{0}_{1\times n} & \boldsymbol{0}_{1\times n}\end{bmatrix}_{1\times2n} & \begin{bmatrix}0 & 0\end{bmatrix}_{1\times2}
\end{bmatrix}_{2\times2n+2}\begin{bmatrix}\left[\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\right]_{2n\times2n} & \left[-\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\right]_{2n\times2}\\
\left[-\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\right]_{2\times2n} & \left[\boldsymbol{W}_{4}^{-1}+\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\right]_{2\times2}
\end{bmatrix}_{2n+2\times2}\\
\left[\boldsymbol{F}_{VH}\boldsymbol{W}^{-1}\right]_{2\times2n+2} & =\begin{bmatrix}\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1} & -\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\\
\begin{bmatrix}\boldsymbol{0}_{1\times n} & \boldsymbol{0}_{1\times n}\end{bmatrix}_{1\times2n} & \begin{bmatrix}0 & 0\end{bmatrix}_{1\times2}
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left[\boldsymbol{F}_{VH}\boldsymbol{W}^{-1}\right]_{2\times2n+2}\left[\boldsymbol{F}_{HV}\right] & =\begin{bmatrix}\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1} & -\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\\
\begin{bmatrix}\boldsymbol{0}_{1\times n} & \boldsymbol{0}_{1\times n}\end{bmatrix}_{1\times2n} & \begin{bmatrix}0 & 0\end{bmatrix}_{1\times2}
\end{bmatrix}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*} & \boldsymbol{0}_{n\times1}\\
\boldsymbol{0}_{n\times1} & \boldsymbol{0}_{n\times1}\\
0 & 0\\
0 & 0
\end{bmatrix}\\
\left[\boldsymbol{F}_{VH}\boldsymbol{W}^{-1}\right]_{2\times2n+2}\left[\boldsymbol{F}_{HV}\right]_{2n+2\times2} & =\begin{bmatrix}\left[\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\right]_{1\times2n} & \left[-\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\right]_{1\times2}\\
\begin{bmatrix}\boldsymbol{0}_{1\times n} & \boldsymbol{0}_{1\times n}\end{bmatrix}_{1\times2n} & \begin{bmatrix}0 & 0\end{bmatrix}_{1\times2}
\end{bmatrix}\begin{bmatrix}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix}_{2n\times1} & \begin{bmatrix}\boldsymbol{0}_{n\times1}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix}_{2n\times1}\\
\begin{bmatrix}0\\
0
\end{bmatrix}_{2\times1} & \begin{bmatrix}0\\
0
\end{bmatrix}_{2\times1}
\end{bmatrix}\\
\left[\boldsymbol{F}_{VH}\boldsymbol{W}^{-1}\boldsymbol{F}_{HV}\right]_{2\times2} & =\begin{bmatrix}\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix} & 0\\
0 & 0
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix} & =\begin{bmatrix}\boldsymbol{\Gamma}_{E} & \boldsymbol{0}_{n\times n}\\
\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E} & \boldsymbol{\Gamma}_{I}
\end{bmatrix}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix}\\
\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix} & =\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\Gamma}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\frac{1}{K_{H}}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}
\end{bmatrix}\\
\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix} & =\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\Gamma}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\frac{1}{K_{H}}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}
\end{bmatrix}\\
\begin{bmatrix}\boldsymbol{0}_{1\times n} & \left(\frac{K_{H}}{K_{B}}\right)\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\end{bmatrix}\left(\boldsymbol{W}_{1}-\boldsymbol{W}_{2}\boldsymbol{W}_{4}^{-1}\boldsymbol{W}_{3}\right)^{-1}\begin{bmatrix}\frac{1}{K_{H}}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\vec{B}^{*}\\
\boldsymbol{0}_{n\times1}
\end{bmatrix} & =\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\left(\frac{\vec{B}^{*}}{K_{B}}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{F}_{VH}\left(\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}\right)^{-1}\boldsymbol{F}_{HV}\boldsymbol{V}_{H}^{-1} & =\begin{bmatrix}\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\left(\frac{\vec{B}^{*}}{K_{B}}\right) & 0\\
0 & 0
\end{bmatrix}\begin{bmatrix}\frac{1}{\mu_{H}+\gamma_{H}} & 0\\
\left(\frac{\gamma_{H}}{\mu_{H}+\gamma_{H}}\right)\frac{1}{\mu_{H}} & \frac{1}{\mu_{H}}
\end{bmatrix}\\
\boldsymbol{F}_{VH}\left(\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}\right)^{-1}\boldsymbol{F}_{HV}\boldsymbol{V}_{H}^{-1} & =\begin{bmatrix}\frac{1}{\mu_{H}+\gamma_{H}}\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\left(\frac{\vec{B}^{*}}{K_{B}}\right) & 0\\
0 & 0
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left|\lambda\boldsymbol{I}-\boldsymbol{K}\right| & =\lambda^{2n+2}\det\left(\lambda^{2}\boldsymbol{I}_{2}-\boldsymbol{F}_{VH}\left(\boldsymbol{V}_{B}-\boldsymbol{V}_{RB}\boldsymbol{V}_{V}^{-1}\boldsymbol{V}_{VR}\right)^{-1}\boldsymbol{F}_{HV}\boldsymbol{V}_{H}^{-1}\right)\\
\left|\lambda\boldsymbol{I}-\boldsymbol{K}\right| & =\lambda^{2n+2}\begin{vmatrix}\lambda^{2}-\frac{1}{\mu_{H}+\gamma_{H}}\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\left(\frac{\vec{B}^{*}}{K_{B}}\right) & 0\\
0 & \lambda^{2}
\end{vmatrix}\\
\left|\lambda\boldsymbol{I}-\boldsymbol{K}\right| & =\lambda^{2n+4}\left(\lambda^{2}-\frac{1}{\mu_{H}+\gamma_{H}}\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\left(\frac{\vec{B}^{*}}{K_{B}}\right)\right)
\end{align*}

\end_inset

Thus 
\begin_inset Formula 
\[
\mathcal{R}_{0}=\frac{1}{\mu_{H}+\gamma_{H}}\vec{1}^{T}\boldsymbol{\beta}_{H}\boldsymbol{\Lambda}_{H}\boldsymbol{\Gamma}_{I}\boldsymbol{\tau}_{E}\boldsymbol{\beta}_{B}\boldsymbol{\Lambda}_{B}\left(\frac{\vec{B}^{*}}{K_{B}}\right)
\]

\end_inset

with 
\begin_inset Formula $\boldsymbol{S}=\vec{\alpha}\left(-\boldsymbol{A}\vec{1}\right)^{T}$
\end_inset

,
 
\begin_inset Formula $\boldsymbol{\Gamma}_{E}=\left[\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S}\right]^{-1}$
\end_inset

,
 
\begin_inset Formula $\boldsymbol{\Gamma}_{I}=\left[\left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}\right]^{-1}$
\end_inset

,
 
\begin_inset Formula $\boldsymbol{\tau}_{E}=\left[\eta\boldsymbol{I}+\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\left(\frac{\gamma_{V}}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\eta}{\mu+\gamma_{R}+\eta}\right)+\left(\frac{\eta}{\mu+\gamma_{V}+\eta}\right)\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\right]\boldsymbol{S}\right]\boldsymbol{\Gamma}_{E}$
\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{S} & =\vec{\alpha}\left(-\boldsymbol{A}\vec{1}\right)^{T}\\
\boldsymbol{\Gamma}_{E} & =\left[\left(\left(\mu+\eta\right)\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}+\eta}\right)\boldsymbol{S}\right]^{-1}\\
\boldsymbol{\Gamma}_{I} & =\left[\left(\mu\boldsymbol{I}-\boldsymbol{A}^{T}\right)-\frac{1}{\mu+\gamma_{V}}\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\boldsymbol{S}\right]^{-1}\\
\boldsymbol{\tau}_{E} & =\left[\eta\boldsymbol{I}+\eta\left(\frac{\gamma_{V}}{\mu+\gamma_{V}}\right)\left(\frac{\gamma_{R}}{\mu+\gamma_{R}}\right)\left[\frac{1}{\mu+\gamma_{R}+\eta}+\frac{1}{\mu+\gamma_{V}}\right]\boldsymbol{S}\right]\boldsymbol{\Gamma}_{E}
\end{align*}

\end_inset


\end_layout

