\begin_body

\begin_layout Section*
Original formulation
\end_layout

\begin_layout Standard
Here we present an alternative mechanistic model of mosquito blood feeding
 dynamics and incorporate transmission.
 
\end_layout

\begin_layout Standard
We assume that blood feeding progress through four discrete stages: questing
 (
\begin_inset Formula $Q$
\end_inset

), landing (
\begin_inset Formula $L$
\end_inset

), probing (
\begin_inset Formula $P$
\end_inset

), and ingesting (
\begin_inset Formula $G$
\end_inset

).
 The mosquito may 
\begin_inset Quotes eld
\end_inset

fail
\begin_inset Quotes erd
\end_inset

 at any of these stages, necessitating a return to an earlier stage.
 We define parameters describing the rate of transition out of a stage (
\begin_inset Formula $\lambda_{\cdot}$
\end_inset

) and parameters describing the probability of successfully transitioning
 out of one stage into the next stage (
\begin_inset Formula $p_{\cdot}$
\end_inset

).
 We additionally define a probability of searching for a new host if a mosquito
 has failed at the landing or probing stages (
\begin_inset Formula $f$
\end_inset

).
 Additionally, using the index 
\begin_inset Formula $i$
\end_inset

, we keep track of whether a mosquito has taken a partial blood meal.
 Assuming that new mosquitoes enter the blood feeding stages from the ovipositio
n and aquatic stages at the rate 
\begin_inset Formula $\rho_{L}L+\gamma V$
\end_inset

, we obtain the following system of equations
\begin_inset Formula 
\begin{align*}
\frac{d}{dt}Q_{1} & =\left\{ \left(\rho_{A}A+\gamma V\right)+f\left[\left(1-p_{L}\right)\lambda_{L}L_{1}+\left(1-p_{P}\right)\lambda_{P}P_{1}\right]\right\} -\lambda_{Q}Q_{1}-\mu Q_{1}\\
\frac{d}{dt}L_{1} & =\left\{ \lambda_{Q}Q_{1}+\left(1-f\right)\left[\left(1-p_{L}\right)\lambda_{L}L_{1}+\left(1-p_{P}\right)\lambda_{P}P_{1}+\left(1-p_{G}\right)\lambda_{G}G_{1}\right]\right\} -p_{l}\lambda_{l}L_{1}-f\left(1-p_{l}\right)\lambda_{l}L_{1}-\mu L_{1}\\
\frac{d}{dt}P_{1} & =p_{l}\lambda_{l}L_{1}-f\left(1-p_{p}\right)\lambda_{p}P_{1}-\left(1-f\right)\left(1-p_{p}\right)\lambda_{p}P_{1}-p_{p}\lambda_{p}P_{1}-\mu P_{1}\\
\frac{d}{dt}G_{1} & =p_{p}\lambda_{p}P_{1}-\left(1-f\right)\left(1-p_{g}\right)\lambda_{g}G_{1}-f\left(1-p_{g}\right)\lambda_{g}G_{1}-p_{g}\lambda_{g}G_{1}-\mu G_{1}\\
\\
\frac{d}{dt}Q_{i} & =\left\{ \iota_{i}\left(\rho_{A}A+\gamma V\right)+\left(1-\iota_{i}\right)f\left(1-p_{G}\right)\lambda_{G}G_{i-1}+f\left[\left(1-p_{L}\right)\lambda_{L}L_{i}+\left(1-p_{P}\right)\lambda_{P}P_{i}\right]\right\} -\lambda_{Q}Q_{i}-\mu Q_{i}\\
\frac{d}{dt}L_{i} & =\left\{ \left(1-\iota_{i}\right)\left(1-f\right)\left(1-p_{G}\right)\lambda_{G}G_{i-1}+\lambda_{Q}Q_{i}+\left(1-f\right)\left[\left(1-p_{L}\right)\lambda_{L}L_{i}+\left(1-p_{P}\right)\lambda_{P}P_{i}+\left(1-p_{G}\right)\lambda_{G}G_{i}\right]\right\} -p_{L}\lambda_{L}L_{i}-f\left(1-p_{L}\right)\lambda_{L}L_{i}-\mu L_{i}\\
\frac{d}{dt}P_{i} & =p_{L}\lambda_{L}L_{i}-f\left(1-p_{P}\right)\lambda_{P}P_{i}-\left(1-f\right)\left(1-p_{P}\right)\lambda_{P}P_{i}-p_{P}\lambda_{P}P_{i}-\mu P_{i}\\
\frac{d}{dt}G_{i} & =p_{P}\lambda_{P}P_{i}-\left(1-f\right)\left(1-p_{G}\right)\lambda_{G}G_{i}-f\left(1-p_{G}\right)\lambda_{G}G_{i}-p_{G}\lambda_{G}G_{i}-\mu G_{i}\\
\\
\frac{d}{dt}V & =\sum_{i=1}^{n}p_{G}\lambda_{G}G_{i}-\gamma V-\mu V
\end{align*}

\end_inset

where 
\begin_inset Formula $\iota_{i}=\begin{cases}
1 & i=1\\
0 & \text{otherwise}
\end{cases}$
\end_inset

.
 These equations can be simplified further to
\begin_inset Formula 
\begin{align*}
\frac{d}{dt}Q_{i} & =\left\{ \iota_{i}\left(\rho_{A}A+\gamma V\right)+\left(1-\iota_{i}\right)f\left(1-p_{G}\right)\lambda_{G}G_{i-1}+f\left[\left(1-p_{L}\right)\lambda_{L}L_{i}+\left(1-p_{P}\right)\lambda_{P}P_{i}\right]\right\} -\left(\lambda_{Q}+\mu\right)Q_{i}\\
\frac{d}{dt}L_{i} & =\left\{ \left(1-\iota_{i}\right)\left(1-f\right)\left(1-p_{G}\right)\lambda_{G}G_{i-1}+\lambda_{Q}Q_{i}+\left(1-f\right)\left[\left(1-p_{P}\right)\lambda_{P}P_{i}+\left(1-p_{G}\right)\lambda_{G}G_{i}\right]\right\} -\left\{ \left[\left(1-f\right)\left(1-p_{L}\right)-p_{L}-f\left(1-p_{L}\right)\right]\lambda_{L}+\mu\right\} L_{i}\\
\frac{d}{dt}P_{i} & =p_{L}\lambda_{L}L_{i}-\left(\lambda_{P}+\mu\right)P_{i}\\
\frac{d}{dt}G_{i} & =p_{P}\lambda_{P}P_{i}-\left(\lambda_{G}+\mu\right)G_{i}\\
\frac{d}{dt}V & =\sum_{i=1}^{n}p_{G}\lambda_{G}G_{i}-\gamma V-\mu V
\end{align*}

\end_inset

where 
\begin_inset Formula $\tilde{\lambda}_{L}=\left[\left(1-f\right)\left(1-p_{L}\right)-p_{L}-f\left(1-p_{L}\right)\right]\lambda_{L}$
\end_inset

.
 
\end_layout

\begin_layout Section*
Approximation
\end_layout

\begin_layout Standard
Assuming that 
\begin_inset Formula $\lambda_{Q},\lambda_{L},\lambda_{P},\lambda_{G}\gg\mu$
\end_inset

, and letting 
\begin_inset Formula $q_{X}=e^{-\mu/\lambda_{X}}$
\end_inset

 for 
\begin_inset Formula $X\in\left\{ Q,L,P,G\right\} $
\end_inset

, we can make the following approximations
\begin_inset Formula 
\begin{align*}
L_{i} & =p_{Q}q_{Q}Q_{i}=r_{Q}Q_{i}\\
P_{i} & =p_{L}q_{L}L_{i}=r_{L}r_{Q}Q_{i}\\
G_{i} & =p_{P}q_{P}P_{i}=r_{P}r_{L}r_{Q}Q_{i}
\end{align*}

\end_inset

where 
\begin_inset Formula $r_{X}=p_{X}q_{X}$
\end_inset

.
 Renaming 
\begin_inset Formula $B_{i}=Q_{i}$
\end_inset

, the sole remaining blood feeding class represented in the model, we can
 then write the system as:
\begin_inset Formula 
\begin{align*}
\frac{d}{dt}B_{i} & =\iota_{i}\left(\rho_{A}A+\gamma V\right)+\left(1-\iota_{i}\right)f\left(1-p_{G}\right)\lambda_{G}r_{P}r_{L}r_{Q}B_{i-1}+f\left[\left(1-p_{L}\right)\lambda_{L}r_{Q}B_{i}+\left(1-p_{P}\right)\lambda_{P}r_{L}r_{Q}B_{i}\right]-\left(\lambda_{Q}+\mu\right)B_{i}\\
L_{i} & =r_{Q}B_{i}\\
P_{i} & =r_{L}r_{Q}B_{i}\\
G_{i} & =r_{P}r_{L}r_{Q}B_{i}\\
\frac{d}{dt}V & =\sum_{i=1}^{n}p_{G}\lambda_{G}r_{P}r_{L}r_{Q}B_{i}-\gamma V-\mu V
\end{align*}

\end_inset

Note that the approximation of 
\begin_inset Formula $L_{i}=r_{Q}B_{i}$
\end_inset

 may not be well-justified because the questing duration may be on the same
 time scale (days) as the mosquito lifespan.
 However, assuming that the questing duration is always less than mosquito
 lifespan, the approximated proportion of surviving mosquitoes 
\begin_inset Formula $r_{Q}=e^{-\mu/\lambda_{Q}}$
\end_inset

 is at most approximately 26% different from the proportion of surviving
 questing mosquitoes at equilibrium 
\begin_inset Formula $\frac{\lambda_{Q}}{\mu+\lambda_{Q}}$
\end_inset

.
 Finally, letting 
\begin_inset Formula $\overline{\lambda}_{Q}=\lambda_{Q}-f\left[\left(1-p_{L}\right)\lambda_{L}r_{Q}+\left(1-p_{P}\right)\lambda_{P}r_{L}r_{Q}\right]$
\end_inset

 and 
\begin_inset Formula $\rho_{Q}=f\left(1-p_{G}\right)r_{P}r_{L}r_{Q}\lambda_{G}$
\end_inset

, we obtain the system
\begin_inset Formula 
\begin{align*}
\frac{d}{dt}L & =\phi V\left(1-\frac{L}{K_{L}}\right)-\left(\rho_{L}+\mu_{L}\right)L\\
\frac{d}{dt}B_{1} & =\left(\rho_{A}L+\gamma V\right)-\left(\overline{\lambda}_{Q}+\mu\right)B_{1}\\
\frac{d}{dt}B_{i} & =\rho_{Q}B_{i-1}-\left(\overline{\lambda}_{Q}+\mu\right)B_{i}\\
L_{i} & =r_{Q}B_{i}\\
P_{i} & =r_{L}r_{Q}B_{i}\\
G_{i} & =r_{P}r_{L}r_{Q}B_{i}\\
\frac{d}{dt}V & =r_{P}r_{L}r_{Q}\sum_{i=1}^{n}B_{i}-\left(\gamma+\mu\right)V
\end{align*}

\end_inset

Let 
\begin_inset Formula $\vec{\alpha}=\left(\alpha_{1},\alpha_{2},\ldots,\alpha_{n}\right)^{T}$
\end_inset

 where 
\begin_inset Formula $\alpha_{i}=\begin{cases}
1 & \textrm{if }i=1\\
0 & \textrm{otherwise}
\end{cases}$
\end_inset

 
\begin_inset Formula 
\begin{align*}
\boldsymbol{A} & =\begin{bmatrix}-\overline{\lambda}_{Q} & \rho_{Q} & \cdots & 0 & 0\\
0 & -\overline{\lambda}_{Q} & \cdots & 0 & 0\\
\vdots & \vdots & \ddots & \vdots & \vdots\\
0 & 0 & \cdots & -\overline{\lambda}_{Q} & \rho_{Q}\\
0 & 0 & \cdots & 0 & -\overline{\lambda}_{Q}
\end{bmatrix}
\end{align*}

\end_inset

Note that we must have 
\begin_inset Formula $\overline{\lambda}_{Q}>0$
\end_inset

 for the transient rate matrix to be well-defined.
 Then the system can be written more compactly as
\begin_inset Formula 
\begin{align*}
\frac{d}{dt}L & =\phi V\left(1-\frac{L}{K_{L}}\right)-\left(\rho_{L}+\mu_{L}\right)L\\
\frac{d}{dt}\boldsymbol{B} & =\left(\rho_{A}L+\gamma V\right)\vec{\alpha}+\boldsymbol{A}^{T}\boldsymbol{B}-\mu\boldsymbol{B}\\
L_{i} & =r_{Q}B_{i}\\
P_{i} & =r_{L}r_{Q}B_{i}\\
G_{i} & =r_{P}r_{L}r_{Q}B_{i}\\
\frac{d}{dt}V & =r_{P}r_{L}r_{Q}\sum_{i=1}^{n}B_{i}-\left(\gamma+\mu\right)V
\end{align*}

\end_inset


\end_layout

\begin_layout Section*
Stability and existence of equilibria
\end_layout

\begin_layout Standard
There exist two equilibria: the extinction equilibrium, 
\begin_inset Formula $\left(L^{E},\boldsymbol{B}^{E},V^{E}\right)=\left(0,\boldsymbol{0},0\right)$
\end_inset

, and a positive equilibrium 
\begin_inset Formula $\left(L^{*},\boldsymbol{B}^{*},V^{*}\right)$
\end_inset

 with values given below.
\end_layout

\begin_layout Standard
For the determination of steady states and stability of the extinction equilibri
um, we calculate the solutions to 
\begin_inset Formula 
\begin{align*}
r & =-\boldsymbol{1}^{T}A^{T}\boldsymbol{B}\\
 & =\begin{bmatrix}\overline{\lambda}_{Q}-\rho_{Q} & \overline{\lambda}_{Q}-\rho_{Q} & \cdots & \overline{\lambda}_{Q}-\rho_{Q} & \overline{\lambda}_{Q}\end{bmatrix}\begin{bmatrix}B_{1}\\
B_{2}\\
\vdots\\
B_{n-1}\\
B_{n}
\end{bmatrix}\\
 & =\rho_{Q}B_{n}+\sum_{i=1}^{n}\left(\overline{\lambda}_{Q}-\rho_{Q}\right)B_{i}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tau & =\left(-\boldsymbol{1}^{T}\boldsymbol{A}^{T}\right)\left(\mu I-\boldsymbol{A}^{T}\right)^{-1}\vec{\alpha}\\
 & =1-\left(\frac{\mu}{\overline{\lambda}_{Q}+\mu}\right)\sum_{i=0}^{n-1}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{i}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\phi\left(1-\frac{L}{K_{L}}\right) & =\left[\frac{1}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}\right]^{-1}\\
L & =K_{L}\left\{ 1-\left[\phi\frac{1}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}\right]^{-1}\right\} 
\end{align*}

\end_inset

and
\begin_inset Formula 
\begin{align*}
\phi V\left(1-\frac{L}{K_{L}}\right) & =\left(\rho_{L}+\mu_{L}\right)L\\
V & =\left(\rho_{L}+\mu_{L}\right)\left[\frac{1}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}\right]K_{L}\left\{ 1-\left[\phi\frac{1}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}\right]^{-1}\right\} \\
V & =K_{L}\left(\rho_{L}+\mu_{L}\right)\frac{1}{\phi}\left[\phi\frac{1}{\gamma+\mu}\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}-1\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
V & =\frac{1}{\gamma+\mu}\left(-\boldsymbol{1}^{T}A^{T}\boldsymbol{B}\right)\\
\rho_{Q}Q_{n}+\sum_{i=1}^{n}\left(\overline{\lambda}_{Q}-\rho_{Q}\right)Q_{i} & =K_{L}\left(\gamma+\mu\right)\left(\rho_{L}+\mu_{L}\right)\frac{1}{\phi}\left[\phi\frac{1}{\gamma+\mu}\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}-1\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{B} & =\left(\rho_{L}L^{*}\left(r\right)+\frac{1}{\gamma+\mu}r\right)\left(\mu I-\boldsymbol{A}^{T}\right)^{-1}\vec{\alpha}\\
\begin{bmatrix}B_{1}\\
B_{2}\\
\vdots\\
B_{n-1}\\
B_{n}
\end{bmatrix} & =\psi\frac{1}{\overline{\lambda}_{Q}+\mu}\begin{bmatrix}1\\
\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)\\
\vdots\\
\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2}\\
\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-1}
\end{bmatrix}
\end{align*}

\end_inset

where 
\begin_inset Formula $\psi=\left(\rho_{L}L^{*}\left(r\right)+\frac{1}{\gamma+\mu}r\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\psi & =\rho_{L}L^{*}\left(r\right)+\frac{1}{\gamma+\mu}r\\
 & =K_{L}\rho_{L}\left[\phi\frac{1}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}\right]^{-1}\left\{ \phi\frac{1}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}-1\right\} +K_{L}\left(\rho_{L}+\mu_{L}\right)\frac{1}{\phi}\left[\phi\frac{1}{\gamma+\mu}\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}-1\right]\\
 & =K_{L}\rho_{L}\frac{1}{R}\left(R-1\right)+K_{L}\left(\rho_{L}+\mu_{L}\right)\frac{1}{\phi}\left(R-1\right)\\
 & =K_{L}\left(\frac{R-1}{R}\right)\left(\rho_{L}+\mu_{L}\right)\frac{1}{\phi}\left(\phi\frac{\rho_{L}}{\rho_{L}+\mu_{L}}+R\right)
\end{align*}

\end_inset

where 
\begin_inset Formula $R=\frac{\phi}{\gamma+\mu}\left(\frac{\rho_{L}}{\rho_{L}+\mu_{L}}\right)\tau\left(1-\tau\frac{\gamma}{\gamma+\mu}\right)^{-1}$
\end_inset

.
 We made use of the following facts: 
\begin_inset Formula 
\begin{align*}
\left(\mu I-\boldsymbol{A}^{T}\right)^{-1} & =\begin{bmatrix}\frac{1}{\overline{\lambda}_{Q}+\mu} & 0 & \cdots & 0 & 0\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right) & \frac{1}{\overline{\lambda}_{Q}+\mu} & \cdots & 0 & 0\\
\vdots & \vdots & \ddots & \vdots & \vdots\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2} & \frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-3} & \cdots & \frac{1}{\overline{\lambda}_{Q}+\mu} & 0\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-1} & \frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2} & \cdots & \frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right) & \frac{1}{\overline{\lambda}_{Q}+\mu}
\end{bmatrix}\\
\left(\mu I-\boldsymbol{A}^{T}\right)^{-1}\vec{\alpha} & =\begin{bmatrix}\frac{1}{\overline{\lambda}_{Q}+\mu} & 0 & \cdots & 0 & 0\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right) & \frac{1}{\overline{\lambda}_{Q}+\mu} & \cdots & 0 & 0\\
\vdots & \vdots & \ddots & \vdots & \vdots\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2} & \frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-3} & \cdots & \frac{1}{\overline{\lambda}_{Q}+\mu} & 0\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-1} & \frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2} & \cdots & \frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right) & \frac{1}{\overline{\lambda}_{Q}+\mu}
\end{bmatrix}\begin{bmatrix}1\\
0\\
\vdots\\
0\\
0
\end{bmatrix}\\
 & =\begin{bmatrix}\frac{1}{\overline{\lambda}_{Q}+\mu}\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)\\
\vdots\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2}\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-1}
\end{bmatrix}\\
-\boldsymbol{1}^{T}\boldsymbol{A}^{T} & =\begin{bmatrix}1 & 1 & \cdots & 1 & 1\end{bmatrix}\begin{bmatrix}\overline{\lambda}_{Q} & 0 & \cdots & 0 & 0\\
-\rho_{Q} & \overline{\lambda}_{Q} & \cdots & 0 & 0\\
\vdots & \vdots & \ddots & \vdots & \vdots\\
0 & 0 & \cdots & \overline{\lambda}_{Q} & 0\\
0 & 0 & \cdots & -\rho_{Q} & \overline{\lambda}_{Q}
\end{bmatrix}\\
 & =\begin{bmatrix}\overline{\lambda}_{Q}-\rho_{Q} & \overline{\lambda}_{Q}-\rho_{Q} & \cdots & \overline{\lambda}_{Q}-\rho_{Q} & \overline{\lambda}_{Q}\end{bmatrix}\\
\left(-\boldsymbol{1}^{T}\boldsymbol{A}^{T}\right)\left(\mu I-\boldsymbol{A}^{T}\right)^{-1}\vec{\alpha} & =\begin{bmatrix}\overline{\lambda}_{Q}-\rho_{Q} & \overline{\lambda}_{Q}-\rho_{Q} & \cdots & \overline{\lambda}_{Q}-\rho_{Q} & \overline{\lambda}_{Q}\end{bmatrix}\begin{bmatrix}\frac{1}{\overline{\lambda}_{Q}+\mu}\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)\\
\vdots\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-2}\\
\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-1}
\end{bmatrix}\\
 & =\overline{\lambda}_{Q}\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{n-1}+\sum_{i=0}^{n-2}\left(\overline{\lambda}_{Q}-\rho_{Q}\right)\frac{1}{\overline{\lambda}_{Q}+\mu}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{i}\\
 & =\sum_{i=0}^{n-1}\left(\frac{\overline{\lambda}_{Q}}{\overline{\lambda}_{Q}+\mu}\right)\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{i}-\sum_{i=1}^{n-1}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{i}\\
 & =1-\left(\frac{\mu}{\overline{\lambda}_{Q}+\mu}\right)\sum_{i=0}^{n-1}\left(\frac{\rho_{Q}}{\overline{\lambda}_{Q}+\mu}\right)^{i}
\end{align*}

\end_inset


\end_layout

\begin_layout Section*
Incorporating transmission
\end_layout

\begin_layout Standard
We now incorporate the possibility of transmission between hosts and vectors.
 We assume that there is no vertical transmission of parasites within the
 mosquito population.
 We assume that the population dynamics of the total host population (
\begin_inset Formula $H$
\end_inset

) are governed by a simple equation: 
\begin_inset Formula $\frac{d}{dt}H=\lambda_{H}-\mu_{H}H$
\end_inset

.
 
\end_layout

\begin_layout Standard
We sub-divide the the host population into susceptible (
\begin_inset Formula $S_{H}$
\end_inset

), infected (
\begin_inset Formula $I_{H}$
\end_inset

), and recovered (
\begin_inset Formula $R_{H}$
\end_inset

) subpopulations.
 This assumes that the intrinsic incubation period of parasite development
 within the vertebrate host is very short relative to the transmission dynamics.
 We further sub-divide the adult vector subpopulations 
\series bold

\begin_inset Formula $\boldsymbol{B}$
\end_inset


\series default
 and 
\begin_inset Formula $V$
\end_inset

 into susceptible (
\begin_inset Formula $B_{i}$
\end_inset

 and 
\begin_inset Formula $V$
\end_inset

), exposed (
\begin_inset Formula $E_{i}$
\end_inset

 and 
\begin_inset Formula $E_{V}$
\end_inset

), and infected (
\begin_inset Formula $I_{i}$
\end_inset

 and 
\begin_inset Formula $I_{V}$
\end_inset

).
 This assumes that, once infected, mosquitoes remain infected for the remainder
 of their lifespan.
 Let 
\begin_inset Formula $\varPhi_{H}$
\end_inset

 and 
\begin_inset Formula $\varPhi_{V}$
\end_inset

 represent the forces of infection to hosts and to vectors, respectively.
 We assume that transmission from hosts to vectors occurs entirely during
 the blood ingestion stages (
\begin_inset Formula $G_{i}$
\end_inset

) and that transmission from vectors to hosts occurs entirely during the
 probing stages (
\begin_inset Formula $P_{i}$
\end_inset

) leading to the force of infection terms, meaning that only the proportion
 of the blood-feeding mosquitoes in the ingestion stage can become exposed
 to the parasite.
 
\end_layout

\begin_layout Standard
We obtain the following system of equations
\begin_inset Formula 
\begin{align*}
\frac{d}{dt}S_{H} & =\lambda_{H}-\varPhi_{H}S_{H}-\mu_{H}H\\
\frac{d}{dt}I_{H} & =\varPhi_{H}S_{H}-\gamma_{H}H-\mu_{H}H\\
\frac{d}{dt}R_{H} & =\gamma_{H}H-\mu_{H}H\\
\\
\frac{d}{dt}L & =\phi V\left(1-\frac{L}{K_{L}}\right)-\left(\rho_{L}+\mu_{L}\right)L\\
\frac{d}{dt}\boldsymbol{B} & =\left(\rho_{A}L+\gamma V\right)\vec{\alpha}-\varPhi_{V}\left(r_{P}r_{L}r_{Q}\boldsymbol{B}\right)+\boldsymbol{A}^{T}\boldsymbol{B}-\mu\boldsymbol{B}\\
\frac{d}{dt}\boldsymbol{E} & =\left(\gamma E_{V}\right)\vec{\alpha}+\varPhi_{V}\left(r_{P}r_{L}r_{Q}\boldsymbol{B}\right)-\eta\boldsymbol{E}+\boldsymbol{A}^{T}\boldsymbol{E}-\mu\boldsymbol{E}\\
\frac{d}{dt}\boldsymbol{I} & =\left(\gamma I_{V}\right)\vec{\alpha}+\eta\boldsymbol{E}+\boldsymbol{A}^{T}\boldsymbol{I}-\mu\boldsymbol{I}\\
L_{i} & =r_{Q}B_{i}\\
P_{i} & =r_{L}r_{Q}B_{i}\\
G_{i} & =r_{P}r_{L}r_{Q}B_{i}\\
\frac{d}{dt}V & =r_{P}r_{L}r_{Q}\sum_{i=1}^{n}B_{i}-\left(\gamma+\mu\right)V\\
\frac{d}{dt}E_{V} & =r_{P}r_{L}r_{Q}\sum_{i=1}^{n}E_{i}-\left(\eta+\gamma+\mu\right)E_{V}\\
\frac{d}{dt}I_{V} & =\eta E_{V}+r_{P}r_{L}r_{Q}\sum_{i=1}^{n}I_{i}-\left(\gamma+\mu\right)I_{V}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
where the forces of infection are given by
\begin_inset Formula 
\begin{align*}
\varPhi_{H} & =\frac{\sum B_{i}}{H}\left(\frac{\sum r_{L}r_{Q}B_{i}}{\sum Q_{i}}\beta_{P}ùúÜ_{P}\right)\frac{\sum I_{i}}{\sum B_{i}}=\left(r_{L}r_{Q}\beta_{P}ùúÜ_{P}\right)\frac{\sum I_{i}}{H}\\
\varPhi_{V} & =\beta_{G}ùúÜ_{G}\frac{I_{H}}{H}
\end{align*}

\end_inset


\end_layout

