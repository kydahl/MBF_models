\begin_layout Subsection*
Reproductive numbers
\end_layout

\begin_layout Standard
The comparisons here are fairly straightforward.
\end_layout

\begin_layout Standard
The reproductive numbers of each model are given by:
\begin_inset Formula 
\begin{align*}
R^{D} & =e\tau^{D}\nu^{D}\\
R^{D} & =e\frac{\tau^{D}}{1-\frac{\gamma}{\gamma+\mu}\tau^{D}}
\end{align*}

\end_inset

where 
\begin_inset Formula $\tau^{D}=\sum_{i=1}^{k}\left(\frac{\lambda_{i}^{D}}{\mu+\lambda_{i}^{D}}\right)\sigma_{i}\prod_{j=1}^{i-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right)$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
R^{F} & =e\sum_{i=1}^{k}\kappa_{i}\nu_{i}^{F}\tau_{i}^{F}\\
R^{F} & =e\sum_{i=1}^{k}\kappa_{i}\frac{\tau_{i}^{F}}{1-\frac{\gamma}{\gamma+\mu}\tau_{i}^{F}}
\end{align*}

\end_inset

where 
\begin_inset Formula $\tau_{i}^{F}=\prod_{j=1}^{i}\frac{\lambda_{i,j}^{F}}{\mu+\lambda_{i,j}^{F}}$
\end_inset

, 
\begin_inset Formula $\nu_{i}^{F}=\frac{1}{1-\frac{\gamma}{\gamma+\mu}\tau_{i}^{F}}$
\end_inset

, and 
\begin_inset Formula $\nu^{F}=\sum_{i=1}^{k}\kappa_{i}\tau_{i}\nu_{i}$
\end_inset

.
\end_layout

\begin_layout Subsection*
Equilibrium proportions
\end_layout

\begin_layout Standard
We also wish to make two other comparisons.
 First, we want to determine the equilibrium proportion of mosquitoes that
 have taken exactly 
\begin_inset Formula $i-1$
\end_inset

 blood meals in a given gonotrophic cycle, which we denote 
\begin_inset Formula $M_{i}$
\end_inset

.
 We'll start with the disruption model.
\end_layout

\begin_layout Subsubsection*
Disruption model
\end_layout

\begin_layout Standard
In this case, it is easy to determine
\begin_inset Formula 
\begin{align*}
M_{i}^{D} & =\left(B_{i}^{D}\right)^{*}\\
 & =\rho_{L}K_{L}\frac{1}{e\tau^{D}}\left(e\tau^{D}\nu^{D}-1\right)\left[\left(\frac{1}{\mu+\lambda_{i}^{D}}\right)\prod_{j=1}^{i-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right)\right]
\end{align*}

\end_inset

Let 
\begin_inset Formula $s=t+r$
\end_inset


\begin_inset Formula 
\begin{align*}
M_{s}^{D} & =M_{t}^{D}\\
\rho_{L}K_{L}\frac{1}{e\tau^{D}}\left(e\tau^{D}\nu^{D}-1\right)\left[\left(\frac{1}{\mu+\lambda_{s}^{D}}\right)\prod_{j=1}^{s-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right)\right] & =\rho_{L}K_{L}\frac{1}{e\tau^{D}}\left(e\tau^{D}\nu^{D}-1\right)\left[\left(\frac{1}{\mu+\lambda_{t}^{D}}\right)\prod_{j=1}^{t-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right)\right]\\
\left(\frac{1}{\mu+\lambda_{s}^{D}}\right)\prod_{j=1}^{s-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right) & =\left(\frac{1}{\mu+\lambda_{t}^{D}}\right)\prod_{j=1}^{t-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right)\\
\left(\frac{1}{\mu+\lambda_{t+r}^{D}}\right)\prod_{j=1}^{t+r-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right) & =\left(\frac{1}{\mu+\lambda_{t}^{D}}\right)\prod_{j=1}^{t-1}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right)\\
\left(\frac{1}{\mu+\lambda_{t+r}^{D}}\right)\prod_{j=t}^{t-1+r}\left(\frac{\lambda_{j}^{D}}{\mu+\lambda_{j}^{D}}\right)\left(1-\sigma_{j}\right) & =\left(\frac{1}{\mu+\lambda_{t}^{D}}\right)\\
\prod_{j=t}^{s-1}\left(1-\sigma_{j}\right) & =\left(\frac{\mu+\lambda_{s}^{D}}{\mu+\lambda_{t}^{D}}\right)\prod_{j=t}^{s-1}\left(\frac{\mu+\lambda_{j}^{D}}{\lambda_{j}^{D}}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
C & =K_{L}\frac{1}{e\tau^{D}}\left(e\tau^{D}\nu^{D}-1\right)\sigma_{1}\left(\frac{1}{\mu+\lambda_{1}^{D}}\right)\\
\sigma_{1} & =\frac{C}{K_{L}\frac{1}{e\tau^{D}}\left(e\tau^{D}\nu^{D}-1\right)}\left(\mu+\lambda_{1}^{D}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection*
Fate model
\end_layout

\begin_layout Standard
This is less straightforward, but not so bad.
 We add up all the mosquitoes which have fed 
\begin_inset Formula $i$
\end_inset

 times.
 This can only occur for mosquitoes of type 
\begin_inset Formula $s$
\end_inset

 where 
\begin_inset Formula $i\le s\le k$
\end_inset

.
\begin_inset Formula 
\begin{align*}
M_{i}^{F} & =\sum_{s=i}^{k}\left(B_{s,i}\right)^{*}\\
 & =\rho_{L}K_{L}\frac{1}{e\nu^{D}}\left(e\nu^{D}-1\right)\left[\sum_{s=i}^{k}\left(\kappa_{s}\nu_{s}\right)\left(\frac{1}{\lambda_{s,i}^{F}}\right)\left(\prod_{r=1}^{i}\frac{\lambda_{s,r}^{F}}{\mu+\lambda_{s,r}^{F}}\right)\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection*
Special case: All 
\begin_inset Formula $\lambda$
\end_inset

's equal
\end_layout

\begin_layout Standard
Now suppose that 
\begin_inset Formula $\lambda_{i}^{D}=\lambda_{i,j}^{F}=\lambda$
\end_inset

 for all 
\begin_inset Formula $1\le j\le i\le k$
\end_inset

.
 Let 
\begin_inset Formula $\xi=\frac{\lambda}{\mu+\lambda}$
\end_inset

 and 
\begin_inset Formula $\bar{\nu}_{i}=\frac{1}{1-\left(\frac{\gamma}{\gamma+\mu}\right)\xi^{i}}$
\end_inset

.
 
\begin_inset Formula $\varsigma_{i}=\sigma_{i}\prod_{j=1}^{i-1}\left(1-\sigma_{j}\right)$
\end_inset

.
 We'll write 
\begin_inset Formula $q=\frac{\gamma}{\gamma+\mu}$
\end_inset

 to save some space.
\end_layout

\end_body
\end_document
