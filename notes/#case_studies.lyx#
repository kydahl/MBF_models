0 & 0 & \cdots & -\lambda_{i,i-1} & \mu+\lambda_{i,i}
\end{bmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\boldsymbol{\omega}_{4}L_{4} & =\begin{bmatrix}\frac{\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}}{\left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)\left(\mu+\lambda_{4,4}\right)-\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}\lambda_{4,4}\frac{\gamma}{\gamma+\mu}} & \frac{\left(\mu+\lambda_{4,1}\right)\lambda_{4,2}\lambda_{4,3}}{\left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)\left(\mu+\lambda_{4,4}\right)-\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}\lambda_{4,4}\frac{\gamma}{\gamma+\mu}} & \frac{\left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\lambda_{4,3}}{\left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)\left(\mu+\lambda_{4,4}\right)-\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}\lambda_{4,4}\frac{\gamma}{\gamma+\mu}} & \frac{\left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)}{\left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)\left(\mu+\lambda_{4,4}\right)-\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}\lambda_{4,4}\frac{\gamma}{\gamma+\mu}}\end{bmatrix}\\
 & =\left(\prod_{j=1}^{4}\frac{1}{\mu+\lambda_{4,j}}\right)n_{4}\begin{bmatrix}\left(\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}\right) & \left(\mu+\lambda_{4,1}\right)\lambda_{4,2}\lambda_{4,3} & \left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\lambda_{4,3} & \left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)\end{bmatrix}\\
 & =\left(\prod_{j=1}^{4}\frac{1}{\mu+\lambda_{4,j}}\right)n_{4}\begin{bmatrix}\left(\lambda_{4,1}\lambda_{4,2}\lambda_{4,3}\right) & \left(\mu+\lambda_{4,1}\right)\lambda_{4,2}\lambda_{4,3} & \left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\lambda_{4,3} & \left(\mu+\lambda_{4,1}\right)\left(\mu+\lambda_{4,2}\right)\left(\mu+\lambda_{4,3}\right)\end{bmatrix}\\
 & =\frac{n_{4}}{\lambda_{4,4}}\begin{bmatrix}\left(\prod_{j=1}^{4}\frac{\lambda_{4,j}}{\mu+\lambda_{4,j}}\right) & \left(\prod_{j=2}^{4}\frac{1}{\mu+\lambda_{4,j}}\right) & \left(\prod_{j=3}^{4}\frac{1}{\mu+\lambda_{4,j}}\right) & 1\end{bmatrix}
\end{align*}

