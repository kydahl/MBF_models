\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\varphi\left(V,L\right) & =\left(\mu_{L}+\rho_{L}\right)\left[\frac{1}{\rho_{L}}\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)-\frac{1}{\rho_{L}}f\left(Q\right)-\frac{1}{\rho_{L}}\frac{\gamma_{R}}{\mu+\gamma_{R}}\frac{\gamma_{V}}{\mu+\gamma_{V}}\left[f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)\right]\right]\\
\bar{\varphi}\left(L\right) & =\frac{1}{V}\left(\mu_{L}+\rho_{L}\right)\left[\frac{1}{\rho_{L}}\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)-\frac{1}{\rho_{L}}f\left(Q\right)-\frac{1}{\rho_{L}}\frac{\gamma_{R}}{\mu+\gamma_{R}}\frac{\gamma_{V}}{\mu+\gamma_{V}}\left[f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)\right]\right]\\
\bar{\varphi}\left(L\right) & =\left(\mu+\gamma_{V}\right)\left(\mu_{L}+\rho_{L}\right)\left[\frac{1}{\rho_{L}}\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)\frac{1}{f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)}-\frac{1}{\rho_{L}}\frac{f\left(Q\right)}{f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)}-\frac{1}{\rho_{L}}\frac{\gamma_{R}}{\mu+\gamma_{R}}\frac{\gamma_{V}}{\mu+\gamma_{V}}\right]\\
L^{*} & =\bar{\varphi}^{-1}\left[\left(\mu+\gamma_{V}\right)\left(\mu_{L}+\rho_{L}\right)\left[\frac{1}{\rho_{L}}\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)\frac{1}{f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)}-\frac{1}{\rho_{L}}\frac{f\left(Q\right)}{f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)}-\frac{1}{\rho_{L}}\frac{\gamma_{R}}{\mu+\gamma_{R}}\frac{\gamma_{V}}{\mu+\gamma_{V}}\right]\right]\\
L^{*} & =j\left(Q\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left(\rho_{L}L+\gamma_{R}R\right)+f\left(Q\right) & =\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)\\
\rho_{L}j\left(Q\right)+\frac{\gamma_{R}}{\mu+\gamma_{R}}\frac{\gamma_{V}}{\mu+\gamma_{V}}\left[f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)\right]+f\left(Q\right) & =\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)\\
j\left(Q\right) & =\left[f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)\right]\left[\frac{1}{\rho_{L}}\bar{\lambda}_{Q}Q\left(\frac{H}{g\left(Q\right)}\right)\frac{1}{f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)}-\frac{1}{\rho_{L}}\frac{f\left(Q\right)}{f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)}-\frac{1}{\rho_{L}}\frac{\gamma_{R}}{\mu+\gamma_{R}}\frac{\gamma_{V}}{\mu+\gamma_{V}}\right]\\
j\left(Q\right) & =\left[f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)\right]q\left(Q\right)\\
\bar{\varphi}^{-1}\left[\left(\mu+\gamma_{V}\right)\left(\mu_{L}+\rho_{L}\right)q\left(Q\right)\right] & =\left[f\left(Q\right)+p_{G}\lambda_{G}G\left(Q\right)\right]q\left(Q\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
